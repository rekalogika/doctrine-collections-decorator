{
    "name": "rekalogika/doctrine-collections-decorator",
    "description": "Lets you easily create decorator classes to dynamically modify the behaviors of Doctrine Collection objects, including the collection objects used by Doctrine ORM in your entities.",
    "homepage": "https://rekalogika.dev/doctrine-collections-decorator",
    "keywords": [
        "doctrine",
        "collection",
        "decorator",
        "trait",
        "abstract",
        "entity",
        "wrapper"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Priyadi Iman Nurcahyo",
            "email": "priyadi@rekalogika.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rekalogika\\Collections\\Decorator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rekalogika\\Collections\\Decorator\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "doctrine/collections": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "phpstan/phpstan": "^1.10",
        "vimeo/psalm": "^5.15",
        "psalm/plugin-phpunit": "^0.18.4",
        "symfony/phpunit-bridge": "^6.3 || ^7.0",
        "symfony/var-dumper": "^6.3 || ^7.0"
    }
}
